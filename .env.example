ENV=development

# API auth
API_KEY=your_api_key_here
ADMIN_API_KEY=

# OpenAI
OPENAI_API_KEY=sk-...

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_anon_or_service_key
DEFAULT_CLIENT_ID=00000000-0000-0000-0000-000000000001

# SendGrid
SENDGRID_API_KEY=SG....
SENDGRID_FROM_EMAIL=you@yourdomain.com
SENDGRID_FROM_NAME=Your Name

# Slack
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...

# LangSmith (optional)
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=lead-qualifier
LANGSMITH_TRACING=false

# Runtime controls
APPROVAL_MODE=true
EMAIL_COOLDOWN_DAYS=7
MAX_TOKENS_PER_RUN=5000
MAX_COST_PER_RUN_USD=0.50
MAX_EXECUTION_TIME_SECONDS=300
MAX_RETRIES_PER_STEP=2

# Queue/worker settings
QUEUE_LEASE_SECONDS=120
QUEUE_MAX_ATTEMPTS=5
WORKER_POLL_SECONDS=2
WORKER_ID=worker-1

# Outbox settings
OUTBOX_POLL_SECONDS=10
OUTBOX_BATCH_SIZE=10

# Prefilter
PREFILTER_BLOCKED_DOMAINS=

# API settings
API_HOST=0.0.0.0
API_PORT=8000
